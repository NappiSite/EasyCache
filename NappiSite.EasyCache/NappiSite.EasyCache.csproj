<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>EasyCache</Title>
    <Company>NappiSite</Company>
    <Copyright>2022</Copyright>
    <PackageProjectUrl>https://github.com/NappiSite/EasyCache</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NappiSite/EasyCache</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <Version>1.4.2</Version>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>nappisite.jpg</PackageIcon>
    <PackageTags>cache;GetOrAddAsync;GetOrAdd;ICacheProvider</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\NappiCloud\nappisite.jpg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Runtime.Caching" Version="7.0.0" />
  </ItemGroup>
</Project>